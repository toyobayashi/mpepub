  style="{{node.attrs.style || ''}}"
  class="domel__body {{node.attrs.class || ''}}"
  wx:elif="{{node.name === 'body'}}">
  <block wx:for="{{node.children}}" wx:key="key">
    <block wx:if="{{item.type === 'text'}}">{{item.text}}</block>
    <domel wx:else node="{{item}}"></domel>
  </block>
</view>

